<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:tns="http://jaxws.clients.com/client"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           version="1.0" targetNamespace="http://jaxws.clients.com/client"
           elementFormDefault="qualified">

    <xs:element name="fetchClientsRequest" type="tns:fetchClientsRequest"/>
    <xs:element name="fetchClientsResponse" type="tns:fetchClientsResponse"/>
    <xs:element name="fetchClientByIdRequest" type="tns:fetchClientByIdRequest"/>
    <xs:element name="fetchClientByIdResponse" type="tns:fetchClientByIdResponse"/>
    <xs:element name="saveClientRequest" type="tns:saveClientRequest"/>
    <xs:element name="saveClientResponse" type="tns:saveClientResponse"/>
    <xs:element name="deleteClientByIdRequest" type="tns:deleteClientByIdRequest"/>
    <xs:element name="deleteClientByIdResponse" type="tns:deleteClientByIdResponse"/>

    <xs:complexType name="fetchClientsRequest">
        <xs:sequence/>
    </xs:complexType>

    <xs:complexType name="fetchClientsResponse">
        <xs:sequence>
            <xs:element name="return" type="tns:clientTO" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="fetchClientByIdRequest">
        <xs:sequence>
            <xs:element name="arg0" type="xs:long" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="fetchClientByIdResponse">
        <xs:sequence>
            <xs:element name="return" type="tns:clientTO" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="saveClientRequest">
        <xs:sequence>
            <xs:element name="arg0" type="tns:clientTO" minOccurs="0"/>
            <xs:element name="arg1" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="saveClientResponse">
        <xs:sequence>
            <xs:element name="return" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="deleteClientByIdRequest">
        <xs:sequence>
            <xs:element name="arg0" type="xs:long" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="deleteClientByIdResponse">
        <xs:sequence/>
    </xs:complexType>

    <xs:complexType name="clientTO">
        <xs:complexContent>
            <xs:extension base="tns:transferObject">
                <xs:sequence>
                    <xs:element name="id" type="xs:long" minOccurs="0"/>
                    <xs:element name="nom" type="xs:string" minOccurs="0"/>
                    <xs:element name="prenom" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="transferObject" abstract="true">
        <xs:sequence/>
    </xs:complexType>


</xs:schema>